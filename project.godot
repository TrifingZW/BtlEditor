; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="BtlEditor"
run/main_scene="res://MainScreen/MainUI.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Globals="*res://CoreScripts/Globals.cs"

[dotnet]

project/assembly_name="BtlEditor"

[editor]

naming/scene_name_casing=1

[gui]

theme/default_theme_scale=2.0
theme/custom_font="res://msyhbd.ttc"

[input_devices]

pointing/android/enable_long_press_as_right_click=true

[internationalization]

locale/fallback="cn"

[rendering]

textures/vram_compression/import_etc2_astc=true
